#include <unistd.io>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

int		ft_strlen(char *str)
{
	int	i;

	i = 0;
	while (str[i])
	{
		i++;
	}
	return (i);
}

int		main(int ac, char **av)
{	
	if (ac == 2)
	{
		
	}
	else
		ft_putchar('\n');
	return (0);
}
